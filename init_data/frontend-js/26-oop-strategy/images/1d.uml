@startuml
class Animal {
Number size
obj canJump
show()
jump()
move()
}

class CanJump {
jump()
}

class CanNotJump {
jump()
}

class Cat {
String name
String image
voice()
}

class Dog {
String name
String image
voice()
}

class WoodenCat {
String name
String image
voice()
}



Animal <|-- Dog
Animal *-- CanJump
Animal *-- CanNotJump

Animal <|-- Dog
Animal <|-- Cat
Animal <|-- WoodenCat

@enduml